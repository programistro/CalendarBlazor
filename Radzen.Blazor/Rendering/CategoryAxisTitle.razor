<g class="rz-axis-title">
    <text x="@X.ToInvariantString()" y="@Y.ToInvariantString()" dominant-baseline="hanging">@Text</text>
</g>

@code {
    [Parameter]
    public double X { get; set; }

    [Parameter]
    public double Y { get; set; }

    [Parameter]
    public string Text { get; set; }
}
